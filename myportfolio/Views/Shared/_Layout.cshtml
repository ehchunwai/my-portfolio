<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Portfolio Eugene</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="~/Views/Testimonial/testimonial.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css">

    <link rel="stylesheet" href="/lib/owlcarousel2/dist/assets/owl.carousel.min.css" />

    <link rel="stylesheet" href="~/css/theme-dark.min.css" />
    <link rel="stylesheet" href="~/css/theme-light.min.css" />
      @await RenderSectionAsync("Styles", required: false)
</head>
<body class="h-100 theme-dark" style="overflow:hidden;">

    <div class="container-fluid p-0 h-100">

        <div class="d-flex h-100">
            <div class="d-flex flex-column p-3 text-white side-bar" style="width: 280px;">
                <img class="img-fluid" src="~/media/profile.jpg" style="border-radius: 50%;" />
                @*<img src="~/media/not-found.png" />*@
                <h5 class="mt-2" style="text-align:center;">Eugene Ho Chun Wai</h5>

                <hr>

                <div class="d-flex justify-content-center mb-2">
                    <span class="material-icons toggle-theme" data-theme="dark" style="cursor: pointer; color:black; display: none;">
                        dark_mode
                    </span>
                    <span class="material-icons ms-2 toggle-theme" data-theme="light" style="cursor:pointer;">
                        light_mode
                    </span>
                </div>

                <ul class="nav nav-pills flex-column mb-auto">
                    <li class="nav-item">
                        <a href="#" class="nav-link active" aria-current="page" data-url="/home/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#home"></use></svg>
                            Home
                        </a>
                    </li>
                    <li>
                        <a href="#" class="nav-link" data-url="/aboutus/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#speedometer2"></use></svg>
                            About Me
                        </a>
                    </li>
                    <li>
                        <a href="#" class="nav-link" data-url="/resume/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"></use></svg>
                            Resume
                        </a>
                    </li>
                    <li>
                        <a href="#" class="nav-link" data-url="/skills/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"></use></svg>
                            Skills
                        </a>
                    </li>
                    <li>
                        <a href="#" class="nav-link" data-url="/projects/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"></use></svg>
                            Projects
                        </a>
                    </li>
                    <li>
                        <a href="#" class="nav-link" data-url="/testimonial/p-index">
                            <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"></use></svg>
                            Testimonial
                        </a>
                    </li>
                </ul>
            </div>

            <div class="h-100 w-100 layout-right-main">
                @RenderBody()
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/owlcarousel2/dist/owl.carousel.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>



    <script src="~/Views/Shared/declaration.js"></script>


    <script src="~/js/main.js" asp-append-version="true"></script>

    <script src="~/Views/Shared/util.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <script src="~/Views/Testimonial/testimonial.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
